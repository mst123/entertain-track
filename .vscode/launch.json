{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "调试 vite 项目",
      "runtimeExecutable": "canary",
      "runtimeArgs": [
        "--auto-open-devtools-for-tabs"
      ],
      // 用户数据目录有个特点，就是只能被一个 Chrome 实例所访问，
      // 如果你之前启动了 Chrome 用了这个默认的 user data dir，那就不能再启动一个 Chrome 实例用它了
      "userDataDir": false,
      "url": "http://localhost:8848",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
